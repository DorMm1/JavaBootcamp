// recursion:
public static int recStairs(int n){
        if((n-2)<0) return 1;
        return  recStairs(n-2) + recStairs(n-1);
    }
    

    public static void main(String[] args) {
        int stairs = 6;
        System.out.println(recStairs(stairs));
// int[] question:
        int[] nums = new int[10];
        for (int i=0; i<nums.length;i++){
            nums[i] = i*i;
        }
        for (int i=0; i<nums.length;i++){
            System.out.println(nums[i]);
        }

    }
